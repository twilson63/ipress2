{
  "name": "svelte-app",
  "version": "1.1.0",
  "devDependencies": {
    "chokidar-cli": "^1.2.2",
    "mdsvex": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-multi-input": "^1.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-svelte": "^5.0.3",
    "svelte": "^3.0.0",
    "w3": "^2.0.0"
  },
  "scripts": {
    "build": "run-s init build:js build:html move:js",
    "init": "rm -rf dist",
    "build:js": "rollup -c",
    "build:html": "node build.js",
    "move:js": "mv scripts/*.js dist/",
    "start": "npx w3 dist",
    "watch": "chokidar \"src/**/*.*\" -c \"npm run build --silent --scripts-prepend-node-path\"",
    "dev": "run-p watch start"
  },
  "dependencies": {
    "klaw-sync": "^6.0.0",
    "ramda": "^0.26.1"
  }
}
